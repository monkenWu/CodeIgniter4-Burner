{
    "name": "monken/codeigniter4-burner",
    "description": "CodeIgniter4 out-of-the-box high-performance server.",
    "keywords": [
		"codeigniter",
		"codeigniter4",
		"RoadRunner",
		"Workerman",
		"high-performance",
		"server",
		"burner"
	],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MonkenWu",
            "homepage": "https://github.com/monkenWu"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2",
        "codeigniter4/framework": "^4.2.10",
        "symfony/var-dumper": "^6",
        "laminas/laminas-diactoros": "^2.8",
        "nyholm/psr7": "^1.5"
    },
    "autoload": {
		"psr-4": {
			"Monken\\CIBurner\\": "src/"
		}
	},
    "require-dev": {
        "codeigniter/coding-standard": "^1.4",
        "rector/rector": "0.12.16"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "composer/package-versions-deprecated": true
        }
    }
}
